{
  "name": "password-manager-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run server",
    "server": "pm2 start ./pm2/process.json && pm2 logs app --lines 0",
    "stop": "pm2 delete ./pm2/process.json"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ],
    "env": {
      "production": {
        "only": [
          "client"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "dependencies": {
    "bcrypt": "0.8.7",
    "body-parser": "~1.15.1",
    "bookshelf": "0.10.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "express-graphql": "0.5.3",
    "express-session": "1.14.0",
    "graphql": "0.6.2",
    "knex": "0.11.9",
    "morgan": "~1.7.0",
    "pug": "2.0.0-beta5",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "serve-favicon": "~2.3.0",
    "sqlite3": "3.1.4"
  },
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.13.0",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "eventsource-polyfill": "0.9.6",
    "file-loader": "0.9.0",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "json-loader": "0.5.4",
    "postcss-cssnext": "2.7.0",
    "postcss-reporter": "1.4.1",
    "pug-loader": "2.2.1",
    "react": "15.3.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  }
}
